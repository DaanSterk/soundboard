<div class="container">
  <br />
  <%= form_for @board, :html => { :multipart => true } do |board| %>
    <div class="field">
      <%= board.label :name %>
      <%= board.text_field :name %>
    </div>
    <div class="field">
      <%= board.submit :class => 'btn btn-success' %>
    </div>
  <% end %>
</div>